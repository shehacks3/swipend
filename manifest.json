{
  "name": "Spndr",
  "version": "1.0",
  "description": "You spend - We help you save",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/back16.png",
      "32": "images/back32.png",
      "48": "images/back48.png",
      "128": "images/back128.png"
    }
  },
  
  "icons": {
    "16": "images/back16.png",
    "32": "images/back32.png",
    "48": "images/back48.png",
    "128": "images/back128.png"
  },
  "manifest_version": 2
}
